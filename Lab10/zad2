import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

public class Main {
    public static void main(String[] args) {
        List<Integer> numbers = new ArrayList<>();
        numbers.add(1);
        numbers.add(2);
        numbers.add(2);
        numbers.add(3);
        numbers.add(5);
        numbers.add(5);
        numbers.add(5);

        System.out.println("Lista przed usunięciem duplikatów: " + numbers);

        List<Integer> uniqueNumbers = removeDuplicates(numbers);
        System.out.println("Lista po usunięciu duplikatów: " + uniqueNumbers);

        List<Integer> duplicatedNumbers = findDuplicates(numbers);
        System.out.println("Zduplikowane elementy w liście: " + duplicatedNumbers);
    }

    public static <E> List<E> removeDuplicates(List<E> list) {
        Set<E> uniqueElements = new HashSet<>(list);
        return new ArrayList<>(uniqueElements);
    }

    public static <E> List<E> findDuplicates(List<E> list) {
        Set<E> uniqueElements = new HashSet<>();
        List<E> duplicatedElements = new ArrayList<>();

        for (E element : list) {
            if (!uniqueElements.add(element)) {
                duplicatedElements.add(element);
            }
        }

        return duplicatedElements;
    }
}
