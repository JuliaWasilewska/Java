public enum Figura {
    DWA("2"),
    TRZY("3"),
    CZTERY("4"),
    PIĘĆ("5"),
    SZÓSTKA("6"),
    SIÓDEMKA("7"),
    ÓSEMKA("8"),
    DZIEWIĄTKA("9"),
    DZIESIĄTKA("10"),
    WALET("Walet"),
    DAMA("Dama"),
    KRÓL("Król"),
    AS("As");

    private final String opis;

    Figura(String opis) {
        this.opis = opis;
    }

    @Override
    public String toString() {
        return opis;
    }
}
public enum Kolor {
    KIER("Kier"),
    KARO("Karo"),
    TREFL("Trefl"),
    PIK("Pik");

    private final String opis;

    Kolor(String opis) {
        this.opis = opis;
    }

    @Override
    public String toString() {
        return opis;
    }
}
public class Karta {
    private final Kolor kolor;
    private final Figura figura;

    public Karta(Kolor kolor, Figura figura) {
        this.kolor = kolor;
        this.figura = figura;
    }

    @Override
    public String toString() {
        return figura + " " + kolor;
    }
}
public class Main {
    public static void main(String[] args) {
        Karta[] talia = new Karta[Kolor.values().length * Figura.values().length];
        int i = 0;
        for (Kolor kolor : Kolor.values()) {
            for (Figura figura : Figura.values()) {
                talia[i] = new Karta(kolor, figura);
                i++;
            }
        }

        for (Karta karta : talia) {
            System.out.println(karta);
        }
    }
}
