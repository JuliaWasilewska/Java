class MojaData {
    private int dzien;
    private int miesiac;
    private int rok;

    public MojaData(int dzien, int miesiac, int rok) {
        this.dzien = dzien;
        this.miesiac = miesiac;
        this.rok = rok;
    }

    @Override
    public String toString() {
      
        String nazwaMiesiaca;
        switch (miesiac) {
            case 1:
                nazwaMiesiaca = "sty";
                break;
            case 2:
                nazwaMiesiaca = "lut";
                break;
            case 3:
                nazwaMiesiaca = "mar";
                break;
            case 4:
                nazwaMiesiaca = "kwi";
                break;
            case 5:
                nazwaMiesiaca = "maj";
                break;
            case 6:
                nazwaMiesiaca = "cze";
                break;
            case 7:
                nazwaMiesiaca = "lip";
                break;
            case 8:
                nazwaMiesiaca = "sie";
                break;
            case 9:
                nazwaMiesiaca = "wrz";
                break;
            case 10:
                nazwaMiesiaca = "pa≈∫";
                break;
            case 11:
                nazwaMiesiaca = "lis";
                break;
            case 12:
                nazwaMiesiaca = "gru";
                break;
            default:
                nazwaMiesiaca = "nieznany";
        }

        return dzien + " " + nazwaMiesiaca + " " + rok;
    }
}

public class Main {
    public static void main(String[] args) {
        MojaData data = new MojaData(1, 11, 2011);

        System.out.println(data.toString());
    }
}
